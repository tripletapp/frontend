{
  "name": "tripletapp-frontend",
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore src",
    "dev:tailwind": "postcss -w src/main.css -o static/main.css",
    "dev:sapper": "sapper dev",
    "dev": "concurrently yarn:dev:*",
    "build:tailwind": "NODE_ENV=production postcss src/main.css -o static/main.css",
    "build:sapper": "sapper build --legacy",
    "build": "yarn build:tailwind && yarn build:sapper",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "autoprefixer": "^9.7.3",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "postcss-import": "^12.0.1",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "sapper": "^0.27.11",
    "svelte": "^3.49.0"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "postcss-cli": "^6.1.3",
    "sirv": "^0.4.0",
    "sirv-cli": "^0.4.4",
    "svelte-zondicons": "^1.0.1",
    "tailwindcss": "^1.1.4",
    "tailwindcss-transitions": "^2.1.0"
  }
}
